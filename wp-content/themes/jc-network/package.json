{
	"name": "jctda",
	"version": "4.0.5",
	"description": "WordPress theme by Rawle Murdy Associates.",
	"homepage": "https://www.discoverjacksonnc.com",
	"license": "private",
	"repository": {
		"type": "git",
		"url": "https://github.com/rawlemurdy/jctda"
	},
	"author": {
		"name": "rawlemurdy",
		"email": "info@rawlemurdy.com",
		"url": "https://rawlemurdy.com",
		"role": "developer"
	},
	"scripts": {
		"start": "npm install && gulp",
		"watch": "gulp watch --silent",
		"build": "gulp",
		"build:release": "npm install && composer install --no-dev -o && gulp",
		"lint": "npm run lint-js && npm run lint-php",
		"lint:js": "eslint assets/js",
		"lint:php": "composer run lint",
		"lint:release": "npm install && composer install && npm run lint",
		"format": "npm run format-js",
		"format:js": "eslint --fix assets/js",
		"test:a11y": "node tests/accessibility/compliance/pa11y.js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint"
		],
		"*.php": [
			"./vendor/bin/phpcs --extensions=php --warning-severity=8 -s"
		]
	},
	"devDependencies": {
		"@10up/component-accordion": "^1.0.0",
		"@10up/component-navigation": "^1.0.0",
		"@10up/component-tabs": "^1.0.0",
		"@10up/eslint-config": "^1.0.6",
		"@10up/stylelint-config": "^1.0.1",
		"@fortawesome/fontawesome-free": "^5.7.1",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.2.6",
		"babel-loader": "^7.1.5",
		"babel-preset-env": "^1.6.1",
		"browser-sync": "^2.24.7",
		"browserslist": "^4.4.1",
		"caniuse-db": "^1.0.30000936",
		"chalk": "^2.4.2",
		"del": "^3.0.0",
		"eslint": "^5.13.0",
		"eslint-loader": "^2.1.2",
		"google-fonts-webpack-plugin": "^0.4.4",
		"gulp": "^4.0.0",
		"gulp-cssnano": "^2.1.2",
		"gulp-filter": "^5.1.0",
		"gulp-postcss": "^8.0.0",
		"gulp-rename": "^1.4.0",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-stylelint": "^7.0.0",
		"husky": "^0.14.3",
		"lint-staged": "^7.2.2",
		"pa11y": "^5.0.4",
		"payment-icons": "^1.1.0",
		"postcss-import": "^12.0.0",
		"postcss-preset-env": "^5.3.0",
		"pump": "^3.0.0",
		"require-dir": "^0.3.2",
		"sanitize.css": "^8.0.0",
		"stylelint": "^9.10.1",
		"stylelint-declaration-use-variable": "^1.7.0",
		"stylelint-order": "^1.0.0",
		"uikit": "^2.27.4",
		"weather-icons": "^1.3.2",
		"webpack": "^4.29.3",
		"webpack-cli": "^3.2.3",
		"webpack-stream": "^5.1.1",
		"webpackbar": "^3.1.5"
	},
	"testing": {
		"urls": {
			"local": "https://jctda.sp.rma.host",
			"staging": "",
			"production": "https://www.discoverjacksonnc.com"
		},
		"accessibility": {
			"compliance": "WCAG2A"
		}
	},
	"browserSync": {
		"proxyUrl": "https://jctda.sp.rma.host",
		"host": "https://jctda.sp.rma.host"
	},
	"engineStrict": true,
	"engines": {
		"node": ">=8.11"
	},
