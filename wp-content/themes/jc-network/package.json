		"lg-fullscreen": "^1.0.1",
		"lg-hash": "^1.0.4",
		"lg-pager": "^1.0.2",
		"lg-thumbnail": "^1.1.0",
		"lg-zoom": "^1.1.0",
		"lightgallery": "^1.6.12",
		"load-google-maps-api": "^1.3.2",
