.location-utility a:hover{color:var(--black);text-decoration:none}
.location-utility a:focus{border:1px solid var(--grey);background-color:var(--grey);color:var(--white)}
div.location-sms{margin-bottom:1rem;padding:1rem;border:3px solid rgba(0, 0, 0, 0.125);background-color:#fff;border-radius:5px}
div.location-meta-value span{font-size:12px;color:#999}
.location-meta-value > a{display:block;padding-right:1rem}
.location-directions{background:#fff;border:1px solid #ccc;box-shadow:0 5px 15px -5px rgba(0, 0, 0, 0.25)}
.location-directions .uk-close{position:absolute;left:calc(100% - 20px);bottom:calc(100% - 20px)}
@media all and (max-width:479px){.contact{display:block}
.contact > div{margin-bottom:0.5rem;padding-bottom:0.5rem;border-bottom:1px solid #eee}}

@media all and (min-width:480px){.contact > div:not(.directions){padding-left:1rem}
.contact > .directions{min-width:160px}}

.location-intro{background:#fff;padding:2rem;border:1px solid #ddd;box-shadow:0 2px 5px rgba(0, 0, 0, 0.25);margin-bottom:2rem}
#single-location-map-frame{height:250px}
.utility-print > a{display:block;padding:1rem;background:#069;color:#fff;text-align:center;font-size:1.25rem}
.location-sms{margin-bottom:1rem;padding:1rem;border:3px solid rgba(0, 0, 0, 0.125);background-color:#fff;border-radius:5px}
@media all and (min-width:960px){.left-column{transform:translateY(-3rem)}}

.overlay{position:relative;padding:3rem;border:1px solid var(--light-grey);box-shadow:0 0.5rem 1rem var(--trans);background-color:var(--white)}
.media-room-lists h3 a:hover{text-decoration:none}
.media-room-lists .uk-list{padding-bottom:1rem;border-bottom:1px dotted var(--light-grey)}
.media-room-lists .uk-list li a{display:block;color:var(--grey)}
.media-room-lists .uk-list a small{display:inline-block;padding:0 5px;background-color:#df7e40}
.media-room-lists .uk-list a small em{color:var(--white);font-style:normal}
.tda-collections .tda-collection{margin-top:2rem;margin-bottom:2rem}
.tda-collections .tda-collection h2{font-size:2rem;line-height:1.1}
.tda-collections .tda-accordion{margin-bottom:1rem}
.tda-collections .tda-accordion .uk-accordion-title{margin:0;padding:0.75rem 1.5rem;border:1px solid var(--blue);border-radius:3px;background-color:var(--blue);color:var(--white)}
.tda-collections .tda-accordion .uk-accordion-title span{float:right}
.tda-collections .tda-accordion .uk-accordion-title .uk-icon-minus{display:none;line-height:1.35}
.tda-collections .tda-accordion .uk-accordion-title .uk-icon-plus{display:inline-block;line-height:1.4}
.tda-collections .tda-accordion .uk-accordion-title.uk-active{border-radius:3px 3px 0 0}
.tda-collections .tda-accordion .uk-accordion-title.uk-active .uk-icon-minus{display:inline-block}
.tda-collections .tda-accordion .uk-accordion-title.uk-active .uk-icon-plus{display:none}
.tda-collections .tda-accordion .uk-accordion-content{padding:0.75rem;border:1px solid transparent}
.tda-collections .tda-accordion .uk-accordion-content.uk-active{border:1px solid var(--light-grey);border-top:0;border-radius:3px}
.tda-collections .tda-accordion .tda-doc h3{margin:0}
.tda-collections .tda-accordion .tda-doc a{margin:0;padding:0.75rem;background-color:var(--white);color:#4f5858;-webkit-transition:var(--transition);transition:var(--transition)}
.tda-collections .tda-accordion .tda-doc a > .item-date{display:inline-block;margin-right:1rem;width:4rem;border-right:1px solid var(--light-grey);font-family:var(--sans-regular);font-size:1.1rem;font-weight:400}
.tda-collections .tda-accordion .tda-doc a > .item-title{color:var(--blue)}
.tda-collections .tda-accordion .tda-doc a:hover,.tda-collections .tda-accordion .tda-doc a:focus{background-color:var(--faint);text-decoration:none}
.tda-collections .tda-accordion .tda-doc a [class*="uk-icon-"]{line-height:1.25}
.thank-you-page .ancillary-content .button{display:inline-block;margin:1% 0}
